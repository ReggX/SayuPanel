{
  "$schema": "../schema/config_schema.json",
  "config_name": "Pokemon Fire Red",
  "events": {
    "file_events": {
      "enabled": true
    },
    "hotkeys": {
      "failsafe": "Shift+Backspace",
      "accept_input": "Shift+]"
    }
  },
  "operators": [
    "$broadcaster",
    "$mods",
    "thundercookie15",
    "reldeththesummoner"
  ],
  "irc": {
    "host": "irc.chat.twitch.tv",
    "port": 6697,
    "message_interval": 3
  },
  "sessionlog": {
    "enable_chatlog": true,
    "enable_consolelog": true
  },
  "teams": [
    {
      "type": "Default",
      "name": "Chat",
      "queue_length": 10,
      "hidden": false,
      "exclusive": false,
      "use_random_inputs": false,
      "channels": [
        "nagzz21",
        "thundercookie15"
      ],
      "spam_protection": true,
      "actionset": {
        "type": "PokemonFireRed_GP",
        "action_prefix": "+",
        "player_index": 0,
        "input_server": {
          "type": "remote",
          "port": 33000
        }
      }
    },
    {
      "type": "Whitelist",
      "name": "Mods",
      "queue_length": 10,
      "hidden": false,
      "use_random_inputs": false,
      "channels": [
        "nagzz21",
        "thundercookie15"
      ],
      "exclusive": false,
      "spam_protection": true,
      "user_whitelist": [
        "$broadcaster",
        "$mods",
        "reldeththesummoner",
        "thundercookie15"
      ],
      "actionset": {
        "type": "PokemonFireRed_KB",
        "action_prefix": "!",
        "player_index": 1,
        "input_server": {
          "type": "remote",
          "port": 33001
        }
      }
    }
  ]
}